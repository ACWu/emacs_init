;; -*- mode: emacs-lisp; coding: utf-8-unix -*-
;; Minibuffer history file, automatically generated by ‘savehist’.

(setq savehist-minibuffer-history-variables '(file-name-history extended-command-history buffer-name-history))
(setq file-name-history '("~/.emacs.d/" "~/.emacs.d/settings.org"))
(setq extended-command-history '("eshell" "dired"))
(setq buffer-name-history '("magit: .emacs.d"))
